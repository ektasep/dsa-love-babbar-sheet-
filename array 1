#include <iostream>

using namespace std;

int main()
{
    
     
         string s;
         cout<<"enter the string";
         cin>>s;
         int n=s.length();
         int temp;
         for(int i=0,j=n-1;i<j;i++,j--){
             temp=s[i];
             s[i]=s[j];
             s[j]=temp;
             
            
         }
         cout<<s; 
        
     
    return 0;
}

